{
  "name": "Pixel Image",
  "version": "1.0.0",
  "description": "Description of the project",
  "main": "server.js",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"nodemon server.js\" \"npm run watch:css\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "npx prettier --write **/*.{js,jsx,md}",
    "format:check": "npx prettier --check **/*.{js,jsx,md}",
    "build:css": "postcss public/css/src/*.css -d public/css/dist/",
    "watch:css": "postcss public/css/src/*.css -d public/css/dist/ --watch"
  },
  "repository": {
    "type": "git",
    "url": "github.com/ideen-los/pixel-image"
  },
  "author": "ideen-los",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cssnano": "^7.0.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "nodemon": "^3.1.7",
    "postcss": "^8.4.49",
    "postcss-cli": "^11.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^4.21.1",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "stripe": "^17.3.1",
    "xss": "^1.0.15"
  }
}
